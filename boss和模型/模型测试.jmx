          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="保存结果到文件" enabled="true">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">String unicode = vars.get(&quot;BText&quot;);
String state = vars.get(&quot;state&quot;);
String prediction = vars.get(&quot;prediction&quot;);

String handleStr=unicode;
//先判断字符里是否含有逗号
if(unicode.contains(&quot;,&quot;)){
    //如果还有双引号，先将双引号转义，避免两边加了双引号后转义错误              
    if(unicode.contains(&quot;\&quot;&quot;)){
        handleStr=unicode.replace(&quot;\&quot;&quot;, &quot;\&quot;\&quot;&quot;);
    }
    //将逗号转义  
    handleStr=&quot;\&quot;&quot;+handleStr+&quot;\&quot;&quot;;  
}
String image = vars.get(&quot;image&quot;);
FileWriter fw = new FileWriter(&quot;E:\\results.csv&quot;,true);
BufferedWriter out = new BufferedWriter(fw);
out.write(image+&quot;,&quot;+handleStr+&quot;,&quot;+state+&quot;,&quot;+prediction);
out.write(System.getProperty(&quot;line.separator&quot;));
log.info(&quot;写完了&quot;);
out.close();
fw.close();</stringProp>
          </BeanShellPostProcessor>
